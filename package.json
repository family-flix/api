{
  "name": "@family-flix/api",
  "version": "2.2.1",
  "private": true,
  "scripts": {
    "dev": "tsx watch ./src/index.ts --port 3200",
    "start": "node dist/server.js",
    "build": "yarn esbuild ./src/index.ts --platform=node --target=node12 --legal-comments=none --bundle --minify --outfile=dist/server.js",
    "db": "npx prisma studio",
    "check": "tsc --noEmit",
    "schedule": "vite-node ./src/schedule/index.ts",
    "lint": "next lint",
    "test": "vitest run",
    "pkg": "node scripts/build && node scripts/pkg",
    "zip": "node scripts/zip"
  },
  "pkg": {
    "assets": [
      "package.json",
      "production/**/*",
      "public/**/*",
      "prisma/**/*"
    ],
    "scripts": [
      "package.json"
    ]
  },
  "dependencies": {
    "@hono/node-server": "^1.11.0",
    "@panva/hkdf": "^1.0.4",
    "@peculiar/webcrypto": "^1.4.3",
    "@prisma/client": "4.13.0",
    "axios": "^1.2.3",
    "chalk": "^5.2.0",
    "cheerio": "^1.0.0-rc.12",
    "cron": "^2.2.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "formidable": "^3.5.1",
    "fuse.js": "^7.0.0",
    "joi": "^17.9.1",
    "jose": "^4.14.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "node-jsencrypt": "^1.0.0",
    "qiniu": "^7.8.0",
    "tencentcloud-sdk-nodejs-asr": "^4.0.642",
    "uuid": "^9.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/cron": "^2.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^9.0.1",
    "@types/xml2js": "^0.4.14",
    "@vercel/ncc": "^0.36.1",
    "archiver": "^5.3.1",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "husky": "^8.0.3",
    "pkg": "^5.8.1",
    "prisma": "^4.14.0",
    "prisma-online": "^0.2.1",
    "resolve": "^1.22.8",
    "ts-morph": "^21.0.1",
    "tsx": "^4.7.2",
    "typescript": "4.9.4",
    "vite-node": "^0.28.3",
    "vitest": "^0.28.2"
  }
}
